      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "eslint": "^8.16.0",
    "eslint-plugin-ft-flow": "^2.0.1",
    "gh-pages": "^4.0.0"
  },
  "parser": "@babel/eslint-parser",
  "plugins": [
    "ft-flow"
  ],
  "rules": {
    "ft-flow/boolean-style": [
      2,
      "boolean"
    ]
  },
  "settings": {
    "ft-flow": {
      "onlyFilesWithFlowAnnotation": false
    }
  }
